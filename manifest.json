{
    "name": "Youtube Skip ads and Mute for Chrome",
    "description": "Auto skip and mute youtube ads",
    "version": "1.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon/icon_16x16.png",
        "48": "icon/icon_48x48.png",
        "128": "icon/icon_128x128.png"

    },
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["skip-ad.js"]
        }
    ],
    "action": {
        "default_title": "Youtube Skip ad for Chrome",
        "default_icon": "icon/icon_48x48.png",
        "default_popup": "popup.html"
    }
}